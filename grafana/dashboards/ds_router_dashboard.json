{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "DS-Router System Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Provider Request Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum by (provider) (increase(provider_requests_total[5m]))",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": [],
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "right"
        }
      }
    },
    {
      "title": "Average Confidence Score",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "avg(confidence_histogram)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "title": "Total Escalations",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(escalations_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "title": "Confidence Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "avg_over_time(confidence_histogram[5m])",
          "legendFormat": "Average Confidence",
          "refId": "A"
        },
        {
          "expr": "quantile_over_time(0.95, confidence_histogram[5m])",
          "legendFormat": "95th Percentile",
          "refId": "B"
        },
        {
          "expr": "quantile_over_time(0.05, confidence_histogram[5m])",
          "legendFormat": "5th Percentile",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 0,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        }
      }
    },
    {
      "title": "Budget and Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Token Usage by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "targets": [
        {
          "expr": "sum by (provider) (rate(oracle_tokens_total[5m]))",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Tokens/sec",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 0,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Cost Tracking (USD)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "targets": [
        {
          "expr": "sum by (provider) (rate(oracle_cost_usd_total[5m]))",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "USD/sec",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 0,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "unit": "currencyUSD"
        }
      }
    },
    {
      "title": "Safety and Circuit Breakers",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Blocked Requests",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 27 },
      "targets": [
        {
          "expr": "sum by (stage) (rate(blocked_requests_total[5m]))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Blocks/sec",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 0,
            "lineWidth": 1,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Provider Health Status",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 27 },
      "targets": [
        {
          "expr": "provider_health_status",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "red",
                  "index": 0,
                  "text": "Unhealthy"
                },
                "1": {
                  "color": "green",
                  "index": 1,
                  "text": "Healthy"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 80 }
            ]
          }
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true
            },
            "indexByName": {},
            "renameByName": {
              "provider": "Provider",
              "Value": "Status"
            }
          }
        }
      ]
    },
    {
      "title": "Request Routing Decisions",
      "type": "logs",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 35 },
      "targets": [
        {
          "expr": "{job=\"liquid-hive-api\"} |= \"routing_decision\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    }
  ],
  "schemaVersion": 27,
  "style": "dark",
  "tags": ["liquid-hive", "ds-router", "ai"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "DS-Router Intelligence Dashboard",
  "uid": "ds-router-v1",
  "version": 1,
  "description": "Real-time monitoring and visualization of the DS-Router v1 intelligent model routing system, including provider usage, confidence scoring, budget tracking, and safety filtering."
}
