# Core toggles
ENABLE_PLANNER=true
ENABLE_ARENA=true
ENVIRONMENT=dev

# Observability (OTEL)
OTEL_SERVICE_NAME=liquid-hive-upgrade
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
OTEL_TRACES_SAMPLER=parentbased_always_on
# ...other OTEL_* as needed

# Backing services
REDIS_URL=redis://redis:6379/0
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=${NEO4J_PASSWORD:?set-in-vault-or-ci}

# Default provider & cost profile
ORACLE_DEFAULT_PROVIDER=deepseek_v3
ROUTING_PROFILE=balanced

# API auth
API_JWT_PUBLIC_KEY_PATH=/run/secrets/api_jwt_pub   # mounted via secrets manager
API_JWT_AUDIENCE=liquid-hive
RATE_LIMIT_RPS=10
TENANCY_MODE=single